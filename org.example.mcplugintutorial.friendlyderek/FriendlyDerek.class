package org.example.mcplugintutorial.friendlyderek;

import org.bukkit.Bukkit;
import org.bukkit.plugin.java.JavaPlugin;
import org.example.mcplugintutorial.friendlyderek.handlers.ZoglinHandler;

public final class FriendlyDerek extends JavaPlugin {
  public void onEnable() {
    Bukkit.getLogger().info("Hello world!");
    new ZoglinHandler(this);
  }
  
  public void onDisable() {
    Bukkit.getLogger().info("Shutting down");
  }
}
